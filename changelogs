local TweenService = game:GetService("TweenService")

local tweenInfo = TweenInfo.new(
     2, -- Duration in seconds
     Enum.EasingStyle.Linear, -- Easing style
     Enum.EasingDirection.InOut -- Easing direction
)

local mainfTween = TweenService:Create(mainf, tweenInfo, {BackgroundTransparency = 0.2})
mainfTween:Play()
wait(3)
local function closeframe()

    changes:Destroy()
    print("NTO; Destroyed ChangeLogs ScreenGui")

end

close.MouseButton1Click:Connect(closeframe)

local function animateTextTypewriter(label, text)
    for i = 1, #text do
        label.Text = string.sub(text, 1, i)
        wait(0.05) -- Adjust the delay as needed
    end
end





local log1txt =  "- Moved the frame to the top right corner."
